{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement", "AwaitInLoop"],
      "no-param-reassign": ["error", { "props": false }],
      "no-console": "off"
    }
}
